	<command>
		<name>What random</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>what</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
			</group>
			<group>
				<word>random</word>
			</group>
		</requiredwords>
		<responses>
			<response>A list of everything that is randomised can be found here (all but experimental ones and DYOM): https://github.com/Parik27/SA.Rainbomizer#features-</response>
		</responses>
	</command>

	<command>
		<name>What chaos mod</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>what</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
			</group>
			<group>
				<word>chaos mod</word>
			</group>
		</requiredwords>
		<responses>
			<response>Chaos Mod triggers random effects every minute, read more here: https://github.com/gta-chaos-mod/Trilogy-ASI-Script</response>
		</responses>
	</command>
	<command>
		<name>Coins reminders</name>
		<dataTrigger type="timer">5-10</dataTrigger>
		<responses>
			<response useName="false" forward="false">/announce You can pool coins for the more expensive options; Don't just spend it all on the cheapest ones!</response>
			<response useName="false" forward="false">/announce You get 69 free coins every 30 minutes! No need to save them for later. :)</response>
		</responses>
	</command>
</commands>