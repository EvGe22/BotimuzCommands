<commands version="1">
	<command>
		<name>Version</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>what</word>
				<word>which</word>
				<word>play</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
			</group>
			<group>
				<word>version</word>
				<word>playing</word>
			</group>
			<group>
				<word>games launcher</word>
				<word>rockstar launcher</word>
				<word wholeword="true">rgl</word>
				<word wholeword="true">steam</word>
				<word wholeword="true">v1</word>
				<word wholeword="true">v2</word>
				<word>need</word>
				<word>mta</word>
			</group>
		</requiredwords>
		<unwantedwords>
			<group>
				<word>epic</word>
				<word wholeword="true">egs</word>
			</group>
		</unwantedwords>
		<responses>
			<response>You need v1 in order to install MTA. You can downgrade any version to v1 though (insert helpful link here)</response>
		</responses>
	</command>
	<command>
		<name>MTA Server</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>what</word>
				<word>which</word>
				<word>where</word>
				<word>name</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
				<word wholeword="true">mta</word>
			</group>
			<group>
				<word>server</word>
				<word wholeword="true">ip</word>
			</group>
		</requiredwords>
		<responses>
			<response>Josh is playing on 123Robot's server, which is a revival of Joshimuz's old server. It's a Derby/Race/Minigame server using the Race gamemode. The address is: mtasa://mta.123robot.me</response>
		</responses>
		<cooldown>5</cooldown>
	</command>
	<command>
		<name>What MTA</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>what</word>
				<word>is this</word>
				<word>means</word>
				<word>name</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
			</group>
			<group>
				<word wholeword="true">mta</word>
				<word wholeword="true">mod</word>
				<word>multi theft auto</word>
			</group>
		</requiredwords>
		<responses>
			<response>MTA stands for Multi Theft Auto. It's a Multiplayer mod for San Andreas.</response>
		</responses>
		<cooldown>5</cooldown>
	</command>
