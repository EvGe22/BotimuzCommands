<commands version="1">
	<command>
		<name>What car</name>
		<requiredwords>
			<group>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
				<word wholeword="true">what</word>
			</group>
			<group>
				<word wholeword="true">car</word>
			</group>
		</requiredwords>
		<responses>
			<response>PROLaps is the Dallara P217, LMP2 class car</response>
		</responses>
	</command>

	<command>
		<name>When Josh Drive?</name>
		<requiredwords>
			<group>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
				<word>when</word>
				<word wholeword="true">how</word>
				<word wholeword="true">who</word>
			</group>
			<group>
				<word>josh</word>
				<word>he</word>
				<word>stint</word>
				<word>plan</word>
				<word>hour</word>
				<word>schedule</word>
			</group>
			<group>
				<word>driv</word>
				<word>turn</word>
				<word>stint</word>
				<word>doing</word>
				<word>race</word>
				<word>racing</word>
				<word>schedule</word>
			</group>
		</requiredwords>
		<responses>
			<response>TIMES ARE IN UTC/GMT AND ARE PROBABLY WRONG OK? https://cdn.discordapp.com/attachments/755316609903493161/863531173976211486/unknown.png</response>
		</responses>
	</command>

	<command>
		<name>What classes</name>
		<requiredwords>
			<group>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
				<word wholeword="true">what</word>
			</group>
			<group>
				<word>class</word>
				<word>colour</word>
			</group>
		</requiredwords>
		<unwantedwords>
			<group>
				<word>flag</word>
			</group>
		</unwantedwords>
		<responses>
			<response>From Fastest to Slowest the classes are: Yellow - LMP1, Blue - LMP2, Pink - GTE</response>
		</responses>
	</command>

